{
  "name": "TestAppTypescript",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "tsc": "tsc",
    "watch": "tsc -w",
    "ios": "npm run build && concurrently -r \"npm run watch\" \"react-native run-ios\"",
    "android": "npm run build && concurrently -r \"start npm run watch\" \"start react-native run-android\"",
    "build": "npm run clean && npm run tsc",
    "clean": "rimraf artifacts"
  },
  "dependencies": {
    "react": "16.3.0-alpha.1",
    "react-native": "0.54.4",
    "typescript": "2.7.2"
  },
  "devDependencies": {
    "@types/react-native": "0.52.13",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "prettier": "1.10.2",
    "concurrently": "3.5.1",
    "rimraf": "2.6.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
